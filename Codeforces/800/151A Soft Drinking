#include<bits/stdc++.h>
using namespace std;
int main(){
 int n, k, l, c, d, p, nl, np;
 cin >> n >> k >> l >> c >> d >> p >> nl >> np;
 int total_drink_ml = k * l;
 int total_lime_slices = c * d;
 int total_salt_grams = p;
 
 int toasts_from_drink = total_drink_ml / (n * nl);
 int toasts_from_limes = total_lime_slices / n;
 int toasts_from_salt = total_salt_grams / (n * np);
 
 int result = min({toasts_from_drink, toasts_from_limes, toasts_from_salt});
 cout << result << endl;
 return 0;
}
